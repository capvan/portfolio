
.user-skills {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: flex-end;

    &::before {
        z-index: -1;
        content: '';
        background-image: url(/src/images/content/bag.png);
        background-size: contain;
        background-repeat: no-repeat;
        height: 100%;
        width: 100%;
        position: absolute;
        bottom: 12.5rem;
        left: -15%;
    }
}