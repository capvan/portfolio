.skill {
    width: 170px;
    height: 170px;
    fill: transparent;
    stroke: #ccc;
    stroke-width: 3px;
    position: relative; 
}

.skill__circle--above {
    stroke-dasharray: 105;
    stroke-dashoffset: 0;
    stroke-width: 15px;
}

.skill__title {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
}

